{
    "manifest_version": 2,

    "name": "PhotoCopy",
    "description": "Copy image data from Filcker to Gettyimage-Beta",
    "version": "1.6",
	"icons": {
      "128": "icon2.png"
	},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "<all_urls>"
    ]
}